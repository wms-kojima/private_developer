<apex:page controller="numeron_Controller" >
<div style="width:317px; height:400px;">
<apex:pageblock >

<div Align="center">

 <apex:image url="data:image/png;base64," alt="数字をえらんてくだちい"/>
 
<div style="background-image:url('https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcQi2F7rNMPkB3MQ3MHKXl3fgEMKVF3Igc_MZn9MnFMgUMgLkBrq'); background-repeat:repeat-y;">

 <apex:form >
 <div style="margin:10px 0px">
 <apex:inputTextArea value="{!input}" /><br/>
 <apex:commandButton style="margin:5px" value="判定！" action="{!onclick}" />
 </div>
 </apex:form>
 
 <div style="width:100px;  padding: 5px； margin-bottom: 10px; border: 1px solid #333333; background-color:#FFFFFF;">
 <p>
  
  <apex:outputText value="{!kekka}"/>
 </p></div>
 
<apex:form >
  <apex:commandButton style="margin:5px" value="答え" action="{!answer}" />
  </apex:form>
<div style="width:100px;  padding: 5px； margin-bottom: 10px; border: 1px solid #333333; background-color:#FFFFFF;">
 <p>
  <apex:outputText value="{!answer}"/>
 </p></div>

 <apex:form >
  <apex:commandButton style="margin:5px" value="もう一回！" action="{!reset}" />
  </apex:form>
</div>

 
</div></apex:pageblock></div>
</apex:page>